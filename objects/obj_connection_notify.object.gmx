            <string>/// Notify 

if server_connection!=global.server_connection
{
    server_connection=global.server_connection
    if server_connection=true
    {
        update_timer=120
    }
    else
    {        
        update_timer=240
    }
    show_debug_message('CHANGED')
}

if update_timer&gt;0
{
    draw_set_alpha(update_timer/30)
    
    if server_connection!=false
    {
        draw_area(130,550,470,600,c_white,c_black,3) 
    }
    else
    {
        draw_area(130,500,470,600,c_white,c_black,3)
    }
    
    draw_config(fnt_connection_notify,c_black,fa_middle,fa_middle)
    
    if server_connection=true
    {    
        draw_text(300,575,'Connected to Server!')
        update_timer-=1
    }
    else if server_connection=false
    {
        draw_text(300,550,"Can't reach server...#Entering Offline Mode!#('R' to retry connection)")
        update_timer-=1
    }
    else if server_connection=undefined
    {
        draw_text(300,575,"Trying to Connect...")
    }
    draw_set_alpha(1) 
}
</string>
