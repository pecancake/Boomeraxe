            <string>/// Debug
if instance_exists(obj_player) and obj_host.gui_state=s.idle and debug_enabled
{
    draw_area(450,450,600,600,c_white,c_black,2)
    draw_config(fnt_debug,c_black,fa_middle,fa_middle)
    draw_text(525,465,'(x,y) : ('+string(obj_player.x)+','+string(obj_player.y)+')')
    draw_text(525,485,'state : ('+string(obj_player.state)+')')
    draw_text(525,505,'kills : ('+string(global.kill_count)+')')
    draw_text(525,525,'deaths : ('+string(global.death_count)+')')
    draw_text(525,545,'jumps : ('+string(global.jump_count)+')')
    draw_text(525,565,'dashes : ('+string(global.dash_count)+')')
    draw_text(525,585,'fps : ('+string(fps)+')')
}

show_debug_overlay(debug_enabled)

if keyboard_check_pressed(vk_delete)
{
    debug_enabled=!debug_enabled
}

if keyboard_check_pressed(vk_home)
{
    room_restart()
    global.k[? 'up']   =vk_up
    global.k[? 'down'] =vk_down
    global.k[? 'left'] =vk_left
    global.k[? 'right']=vk_right
    global.k[? 'jump'] =ord('Z')
    global.k[? 'throw']=ord('X')
    global.k[? 'dash'] =vk_shift
}

if keyboard_check_pressed(vk_insert)
{
    player_invincible=!player_invincible
}

if instance_exists(obj_player)
{
    if player_invincible=true
    {
        obj_player.image_alpha=0.4
        obj_player.move_speed=12
        
        if mouse_check_button_pressed(mb_left)
        {
            obj_player.x=mouse_x
            obj_player.y=mouse_y
        }
        x=mouse_x
        y=mouse_y
    }
    else
    {
        obj_player.image_alpha=1
        obj_player.move_speed=6
    }
}

if keyboard_check_pressed(vk_end)
{
    obj_host.run_over=true
}

if mouse_check_button_pressed(mb_right)
{
    repeat(20)
    {
        ID=instance_create(mouse_x,mouse_y,obj_player_death_new)
        ID.direction=random(360)
        ID.speed=random_range(1,8)
        ID.dest_x=obj_player.x
        ID.dest_y=obj_player.y
        ID.scale=random_range(1.5,3)
        ID.colour=merge_colour(choose(c_maroon,c_navy),c_white,random_range(0.2,0.4))
    }
    create_particles(mouse_x,mouse_y,'death_end')
}
