        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Read run data

ini_open('boomeraxe.ini')

best_time=ini_read_real('meta','best',2000)
profile_count=ini_read_real('meta','profiles',1)

profiles=ds_grid_create(profile_count,10)

for (i=0;i!=profile_count;i++)
{
    name=ini_read_string('meta',string(i),'')
    
    profiles[# i,0]=name
    profiles[# i,1]=ini_read_real(name,'best',2000)
    profiles[# i,2]=ini_read_real(name,'run_count',0)
    
    ds_grid_resize(profiles,max(profiles[# i,2],ds_grid_width(profiles)),ds_grid_height(profiles))
    
    for (z=3;z!=3+profiles[# i,2];z++)
    {
        profiles[# i,z]=ini_read_real(name,string(z),0)
    }
    
    profiles[# i,3+profiles[# i,2]+0]=ini_read_real(name,'up',vk_up)
    profiles[# i,3+profiles[# i,2]+1]=ini_read_real(name,'down',vk_down)
    profiles[# i,3+profiles[# i,2]+2]=ini_read_real(name,'left',vk_left)
    profiles[# i,3+profiles[# i,2]+3]=ini_read_real(name,'right',vk_right)
    profiles[# i,3+profiles[# i,2]+4]=ini_read_real(name,'jump',ord('Z'))
    profiles[# i,3+profiles[# i,2]+5]=ini_read_real(name,'throw',ord('X'))
    profiles[# i,3+profiles[# i,2]+6]=ini_read_real(name,'dash',vk_shift)
}

ini_close()

for (i=0;i!=ds_grid_height(profiles);i++)
{
    for (z=0;z!=ds_grid_width(profiles);z++)
    {
        show_debug_message(string(profiles[# z,i]))
    }
}

game_end()

</string>
